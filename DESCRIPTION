Package: semicor
Type: Package
Title: Correlation Estimation for Maximum Achievable Predictive Accuracy (MAPA)
Version: 0.1.0
Author: c(person('Ishaan', 'Gadiyar', email='ishaan.v.gadiyar@Vanderbilt.Edu', role=c('aut', 'cre')),
  person('Megan', 'Jones', email = 'megan.n.taylor@vanderbilt.edu', role = c('aut')),
